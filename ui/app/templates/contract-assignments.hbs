<h2>Assignments</h2>

<CheckEmpty
  @count={{this.model.meta.count}}
  noResultsMessage="No assignments as of yet"
>
  <Pagination
    @totalCount={{this.model.meta.count}}
    @singularName="assignment"
  />
  <ContractAssignmentsList @assignments={{this.assignments}} @enrollments={{this.enrollments}} @getNotes={{route-action "getNotes"}} />
</CheckEmpty>