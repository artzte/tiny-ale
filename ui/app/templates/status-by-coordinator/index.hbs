<CheckEmpty
  @count={{this.students.length}}
  @noResultsMessage="No coordinatees"
>    
  <Pagination
    @totalCount={{this.students.length}}
    @singularName="coordinatee"
  />

  <StatusByCoordinator
    @students={{this.students}}
    @term={{this.term}}
    @statuses={{this.statuses}}
  />
</CheckEmpty>
