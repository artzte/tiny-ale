<CheckEmpty
  @count={{this.model.meta.count}}
  @noResultsMessage="No students were found with these filters"
>
  <Pagination 
    @offset={{this.offset}}
    @limit={{this.limit}}
    @totalCount={{this.model.meta.count}}
    @singularName="student"
  />
  <StudentsList @students={{this.students}} />
  <Pagination 
    @offset={{this.offset}}
    @limit={{this.limit}}
    @totalCount={{this.model.meta.count}}
    @singularName="student"
  />
</CheckEmpty>
