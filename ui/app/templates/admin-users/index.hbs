{{#check-empty
  count=this.model.meta.count
  noResultsMessage="No users were found with these filters"
}}
  <Pagination
    @totalCount={{this.model.meta.count}}
    @offset={{this.offset}}
    @limit={{this.limit}}
    @singularName="user"
  />
  <UsersList @users={{this.users}} @logins={{this.logins}} />
  <Pagination
    @totalCount={{this.model.meta.count}}
    @offset={{this.offset}}
    @limit={{this.limit}}
    @singularName="user"
  />
{{/check-empty}}
