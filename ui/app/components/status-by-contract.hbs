<Table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Status</th>
      {{#each months as |month|}}
        <th data-test-term-month title=<DateTime @date={{month}} @format= "compactTermMonth" />>
          <DateTime @date={{month}} @format= "MMM" />
        </th>
      {{/each}}
    </tr>
  </thead>
  {{#each enrollments as |enrollment|}}
    <StatusByContract::Row @enrollment={{enrollment}} @statusHash={{statusHash}} @notesHash={{notesHash}} @months={{months}} />
  {{/each}}
</Table>
