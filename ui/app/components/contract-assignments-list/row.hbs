<tr class="contract-assignments-list-participant">
  <td>
    {{full-name student}}
  </td>
  {{#each assignments as |assignment|}}
    <ContractAssignmentsList::Column @enrollment={{enrollment}} @assignment={{assignment}} @turninsHashedByAssignment={{turninsHashedByAssignment}} />
  {{/each}}
</tr>
<tr class="notes-row {{loadingNotes}}">
  <td colspan="3">
    <NotesList @notes={{notes}} @expanded={{notesExpanded}} />
  </td>
</tr>