<thead>
  <tr>
    <th class="class">Class</th>
    <th class="status">Status</th>
    {{#each months as |month|}}
      <th class="month">{{t-date month "compactTermMonth"}}</th>
    {{/each}}
  </tr>
</thead>

{{#each enrollmentsList as |enrollment|}}
  <StatusByStudentEnrollments::Row @enrollment={{enrollment}} @statusHash={{statusHash}} @notablesHash={{notablesHash}} @months={{months}} />
{{/each}}
