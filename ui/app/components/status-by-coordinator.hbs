<thead>
  <tr>
    <th>Name</th>
    <th>Status</th>
    {{#each months as |month|}}
      <th data-test-term-month title={{t-date month "compactTermMonth"}}>{{t-date month "MMM"}}</th>
    {{/each}}
  </tr>
</thead>
<tbody>
  {{#each students.data as |student|}}
    <StatusByCoordinator::Row @student={{student}} @statusHash={{statusHash}} @months={{months}} @term={{term}} />
  {{else}}
    <tr class="no-results">
      <td>No coordinatees</td>
    </tr>
  {{/each}}
</tbody>