<dt>Status</dt>
<dd data-test-enrollment-status>{{this.enrollmentStatus}}</dd>
<dt>Credits</dt>
<dd>
  <CreditAssignments @creditAssignments={{this.creditAssignments}} @creditable={{this.enrollment}} />
</dd>
<dt>Assignments</dt>
<dd data-test-assignments-status>
  {{#if this.assignments}}
    Completed
    {{this.completedAssignments.length}}
    of
    {{this.assignments.length}}
  {{else}}
    No assignments defined for this contract.
  {{/if}}
</dd>
<dt>Attendance</dt>
<dd data-test-attendance-status>
  {{#if this.meetings}}
    Attended
    {{this.attendedMeetings.length}}
    of
    {{this.meetings.length}}
  {{else}}
    No attendance defined for this contract.
  {{/if}}
</dd>
